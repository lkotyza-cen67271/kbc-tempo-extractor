{
	"type": "object",
	"title": "extractor configuration",
	"required": [
		"org_name", "user_email", "#jira_token", "#tempo_token", "since", "datasets"
	],
	"properties": {
		"debug": {
			"type": "boolean",
			"title": "Debug:",
			"default": "False",
			"propertyOrder": 1000
		},
		"incremental": {
			"type": "boolean",
			"title": "Incremental:",
			"default": "False",
			"propertyOrder": 900
		},
		"org_name": {
			"type": "string",
			"title": "Organization name:",
			"description": "https://[org].atlassian.net",
			"propertyOrder": 1
		},
		"user_email": {
			"type": "string",
			"title": "User email:",
			"propertyOrder": 2
		},
		"#jira_token": {
			"type": "string",
			"title": "Jira token:",
			"format": "password",
			"propertyOrder": 3
		},
		"#tempo_token": {
			"type": "string",
			"title": "Tempo token:",
			"format": "password",
			"propertyOrder": 4
		},
		"since": {
			"type": "string",
			"title": "Since:",
			"description": "'1 min ago', '2 weeks ago', '3 months, 1 week and 1 day ago', 'in 2 days', 'tomorrow'",
			"propertyOrder": 5
		},
		"datasets": {
			"type": "array",
			"format": "select",
			"title": "Datasets:",
			"description": "",
			"uniqueItems": true,
			"items": {
				"options": {
					"enum_titles": [
						"Worklog Authors",
						"Approvals",
						"Teams and Membership"
					]
				},
				"enum": [
					"worklog_authors",
					"approvals",
					"teams"
				],
				"type": "string"
			},
			"default": [
				"worklog_authors"
			],
			"propertyOrder": 6
		}
	}
}
